   "source": [
    "select pd.name, pv.name, pd.price\n",
    "from ((products pd inner join providers pv on pd.id_providers=pv.id)\n",
    "inner join categories cat on pd.id_categories=cat.id)\n",
    "where pd.price > 1000 and cat.name = 'Super Luxury';"
   ]
  }
